package com.creativeprograms;

public class SecondLargestNumber {
	public static void main(String[] args) {
		int [] arr = {15,58,10,36,45};
		int i, firstNo, secondNo;
		
		for (i = 0; i < arr.length; i++) {
			firstNo = 0;
			if(arr[i] > firstNo) {
				firstNo = arr[i];
			}
			System.out.println(firstNo);
		}
		
	}
}
